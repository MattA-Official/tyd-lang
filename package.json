{
    "name": "tyd-lang",
    "displayName": "TyD",
    "description": "Syntax highlighting for Tynan's Tidy Data Language",
    "publisher": "Matthew-Adcock",
    "version": "1.4.0",
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#0C585B",
        "theme": "light"
    },
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tyd",
                "aliases": [
                    "TyD",
                    "tyd"
                ],
                "extensions": [
                    ".tyd"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "ttyd",
                "aliases": [
                    "TTyD",
                    "ttyd"
                ],
                "extensions": [
                    ".ttyd"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tyd",
                "scopeName": "source.tyd",
                "path": "./syntaxes/tyd.tmLanguage.json"
            },
            {
                "language": "ttyd",
                "scopeName": "source.tyd",
                "path": "./syntaxes/tyd.tmLanguage.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MattA-Official/tyd-lang"
    }
}
