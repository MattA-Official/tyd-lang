{
    "name": "tyd-lang",
    "displayName": "TyD",
    "description": "Syntax highlighting for Tynan's Tidy Data Language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "scripts": {
        "package": "npx vsce package"
    },
    "contributes": {
        "languages": [{
            "id": "tyd",
            "aliases": ["TyD", "tyd"],
            "extensions": [".tyd"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tyd",
            "scopeName": "source.tyd",
            "path": "./syntaxes/tyd.tmLanguage.json"
        }]
    },
    "dependencies": {
        "npx": "10.2.2",
		"vscode-extension-telemetry": "~0.1.2"
	}
}