{
	"name": "tyd-lang",
	"displayName": "TyD",
	"description": "Syntax highlighting for Tynan's Tidy Data Language",
	"publisher": "Matthew-Adcock",
	"version": "1.5.0",
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#0C585B",
		"theme": "light"
	},
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "TyD",
				"aliases": [
					"tyd"
				],
				"extensions": [
					".tyd",
					".ttyd"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "TyD",
				"scopeName": "source.tyd",
				"path": "./syntaxes/tyd.tmLanguage.json"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/MattA-Official/tyd-lang"
	}
}
