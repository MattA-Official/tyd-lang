{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "TyD",
	"patterns": [{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#paren-expression"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#units"
		},
		{
			"include": "#operators"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.tyd",
				"match": "(\\*[a-zA-Z]*)"
			}]
		},
		"strings": {
			"patterns": [{
				"name": "constant.character.escape",
				"match": "\\\\[.]"
			}, 
			{
				"name": "string.quoted.double",
				"match": "(\"[^\"\\\\]*(?:\\\\.[^\"\\\\]*)*\")"
			}
		]
		},
		"comments": {
			"patterns": [{
				"name": "comment",
				"begin": "#",
				"end": "$"
			}, {
				"name": "comment.block.documentation",
				"begin": "###",
				"end": "$"
			}]
		},
		"units": {
			"name": "keyword.other.unit.tyd",
			"match": "(\\d)"
		},
		"operators": {
			"name": "keyword.operator.tyd",
			"match": "(\\|)"
		}
	},
	"scopeName": "source.tyd"
}