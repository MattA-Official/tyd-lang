{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "TyD",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#operators"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.tyd",
					"match": "(\\*[a-zA-Z]*)"
				}
			]
		},
		"strings": {
			"patterns": [
				{
					"name": "constant.character.escape",
					"match": "\\\\[.]"
				},
				{
					"name": "string.quoted.double",
					"match": "(\"[^\"\\\\]*(?:\\\\.[^\"\\\\]*)*\")"
				},
				{
					"name": "string.unquoted",
					"match": "([\\w][^\\\"\\[\\{\\*\\s]*[\\n\\;\\]\\}\\#])|([\\w][^\\\"\\[\\{\\*\\s]*[\\s])(?:[\\S]+?)"
				},
				{
					"name": "string.other",
					"match": "\\d"
				}
			]
		},
		"comments": {
			"patterns": [
				{
					"name": "comment",
					"begin": "#",
					"end": "$"
				},
				{
					"name": "comment.block.documentation",
					"begin": "###",
					"end": "$"
				}
			]
		},
		"operators": {
			"name": "keyword.operator.tyd",
			"match": "(\\|)"
		}
	},
	"scopeName": "source.tyd"
}