{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "TyD",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#paren-expression"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.tyd",
				"match": "[*|]"
			}]
		},
		"strings": {
			"name": "string.quoted.double.tyd",
			"begin": "\"",
			"end": "\"",
			"patterns": [{
				"name": "constant.character.escape.tyd",
				"match": "\\\\."
			}]
		},
		"comments": {
			"name": "comment",
			"begin": "#",
			"end": "$"
		}

	},
	"scopeName": "source.tyd"
}